    <section class="section" id="funkcje">
        <div class="container-fluid">
            <div class="row justify-content-center">
                <div class="col-lg-5 text-center">
                    <div class="title text-center mb-5">
                        <h1 class="text-dark">Statystyki</h1>
                    </div>

                </div>
            </div>
            <div class="row">
                    <div class="col-md-4">
                        <div class="card">
                            <div class="card-body">
                                <div class="row">
                                    <h3>Sklepy</h3>
                                    <h1 style="color: black;"><%= stat %></h1>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="card">
